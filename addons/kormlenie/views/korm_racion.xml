            <form string="Форма Рацион кормления">
                <sheet>
                    <group>
                        <group>
                            <field name="stado_fiz_group_id"/>
                        </group>
                        <group>
                            <field name="date"/>
                        </group>
                     </group>
                     <group>
                        <separator string="Продуктивность" colspan="2"/>
                        <group>
                            <field name="milk"/>
                            <field name="massa"/>
                        </group>
                        <group>
                            <field name="jir"/>
                            <field name="belok"/>
                        </group>
                     </group>
                   
            
                       
