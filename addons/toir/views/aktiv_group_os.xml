<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
    	<record model="ir.ui.view" id="aktiv_group_os_form_view">
            <field name="name">aktiv_group_os.form</field>
            <field name="model">aktiv.group_os</field>
            <field name="arch" type="xml">
                <form string="Форма Группа ОС (бух)">
                    <sheet>
                        <group>
                            <field name="name" string="Наименование"/>
                           
                        </group>
    		            
                    </sheet>
                </form>
            </field>
        </record>
	
	
	   <record model="ir.ui.view" id="aktiv_group_os_search_view">
            <field name="name">aktiv_group_os.search</field>
            <field name="model">aktiv.group_os</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name" string="Наименование"/>
                    
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="aktiv_group_os_list_action">
            <field name="name">Группа ОС (бух)</field>
            <field name="res_model">aktiv.group_os</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Создайте первую запись Группа ОС (бух)
                </p>
            </field>
        </record>


    </data>
</openerp>
